{
  "name": "ophanim-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:public": "ng serve --host 0.0.0.0",
    "build": "NODE_OPTIONS=${NODE_OPTIONS:=--max_old_space_size=4096} ng build",
    "build:dev": "npm run build",
    "build:qa": "npm run build -- --configuration=qa",
    "build:beta": "npm run build -- --configuration=beta",
    "build:docker": "npm run build -- --configuration=docker",
    "build:k8s": "npm run build -- --configuration=k8s",
    "build:oph3": "npm run build -- --configuration=oph3",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix=true",
    "tslint": "tslint --project ./src/tsconfig.app.json",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "format:check": "prettier --check \"**/*.{ts,js,json,css,scss,md,html}\"",
    "format:fix": "prettier --write",
    "format:fix:all": "npm run format:fix -- \"**/*.{ts,js,json,css,scss,md,html}\"",
    "postinstall": "rm -f ./node_modules/ngx-cron-jobs/ngx-cron-jobs.es5.js",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.5",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.0.5",
    "@angular/compiler": "^11.0.5",
    "@angular/core": "^11.0.5",
    "@angular/forms": "^11.0.5",
    "@angular/material": "^11.0.3",
    "@angular/material-moment-adapter": "^11.0.3",
    "@angular/platform-browser": "^11.0.5",
    "@angular/platform-browser-dynamic": "^11.0.5",
    "@angular/platform-server": "^11.0.5",
    "@angular/router": "^11.0.5",
    "@fullcalendar/angular": "^5.4.0",
    "@fullcalendar/daygrid": "^5.4.0",
    "@fullcalendar/interaction": "^5.4.0",
    "@fullcalendar/list": "^5.4.0",
    "@fullcalendar/timegrid": "^5.4.0",
    "@fullcalendar/timeline": "^5.4.0",
    "@ngrx/effects": "^10.1.1",
    "@ngrx/entity": "^10.1.1",
    "@ngrx/router-store": "^10.1.1",
    "@ngrx/store": "^10.1.1",
    "@ngrx/store-devtools": "^10.1.1",
    "@sentry/browser": "^5.29.1",
    "core-js": "^3.8.1",
    "cronstrue": "^1.106.0",
    "moment": "^2.29.1",
    "ng5-slider": "^1.2.4",
    "node-sass": "^5.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.3",
    "xlsx": "^0.16.9",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.5",
    "@angular/cli": "^11.0.5",
    "@angular/compiler-cli": "^11.0.5",
    "@angular/language-service": "^11.0.5",
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.1.11",
    "@storybook/angular": "^6.1.21",
    "@storybook/cli": "^6.1.11",
    "@types/chai": "^4.2.14",
    "@types/cucumber": "^6.0.1",
    "@types/jasmine": "~3.6.2",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.14.14",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "codelyzer": "^6.0.1",
    "cucumber": "^6.0.5",
    "cypress": "^6.1.0",
    "husky": "^4.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "^10.5.3",
    "prettier": "2.4.1",
    "rxjs-tslint": "^0.1.8",
    "storybook-addon-paddings": "^3.1.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.5",
    "webpack": "^4.44.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.{ts,js,json,css,scss,md,html}": [
      "npm run format:fix"
    ],
    "**/!(*.{spec,stories}).ts": [
      "npm run tslint --no-verify"
    ]
  }
}